{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "@vercel/python@3.12",
      "includeFiles": [
        "templates/**",
        "assets/**",
        "images/**"
      ]
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/api/index.py" }
  ]
}
