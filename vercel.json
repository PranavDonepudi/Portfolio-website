{
  "version": 2,
  "functions": {
    "api/**/*.py": { "runtime": "python3.11" }
  },
  "routes": [
    { "src": "/(assets|images|css|js|static)/(.*)", "dest": "/$1/$2" },
    { "src": "/projects", "dest": "/api/index.py" },
    { "src": "/experience", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
